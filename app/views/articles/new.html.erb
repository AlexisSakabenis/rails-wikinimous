<h1> Create Article </h1>
<%= link_to 'Back to articles', articles_path %>

<%= render 'form', article: @article %>
<%= form_with(model: @article) do |form| %>
  <%= form.label :title %><br/>
  <%= form.text_field :title %><br/>

  <%= form.label :content %><br/>
  <%= form.text_field :content %><br/>

  <%= form.submit %>
<% end %>
